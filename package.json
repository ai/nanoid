{
  "name": "nanoid",
  "version": "0.2.2",
  "description": "A tiny (179 bytes), secure URL-friendly unique string ID generator",
  "keywords": [
    "uuid",
    "random",
    "id",
    "url"
  ],
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "repository": "ai/nanoid",
  "browser": {
    "./random.js": "./random-browser.js"
  },
  "devDependencies": {
    "benchmark": "^2.1.4",
    "chalk": "^2.1.0",
    "docdash": "^0.4.0",
    "eslint": "^4.8.0",
    "eslint-config-logux": "^16.2.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-es5": "^1.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "jsdoc": "^3.5.5",
    "lint-staged": "^4.2.3",
    "microtime": "^2.1.6",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.2",
    "shortid": "^2.2.8",
    "size-limit": "^0.11.6",
    "uuid": "^3.1.0",
    "webpack-dev-server": "^2.9.1",
    "yaspeller-ci": "^0.7.0"
  },
  "scripts": {
    "lint-staged": "lint-staged",
    "spellcheck": "yarn docs && yaspeller-ci *.md docs/*.html",
    "clean": "rimraf docs/ coverage/",
    "docs": "jsdoc --configure .jsdocrc *.js",
    "lint": "eslint *.js test/*.js",
    "test": "jest --coverage && yarn lint && size-limit && yarn spellcheck",
    "demo": "webpack-dev-server --config test/demo/webpack.config"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 100
      }
    }
  },
  "eslintConfig": {
    "extends": "eslint-config-logux/browser"
  },
  "size-limit": [
    {
      "path": "index.js",
      "limit": "179 B"
    },
    {
      "path": "generate.js",
      "limit": "184 B"
    }
  ],
  "lint-staged": {
    "*.md": "yaspeller-ci",
    "*.js": "eslint"
  },
  "pre-commit": [
    "lint-staged"
  ]
}
